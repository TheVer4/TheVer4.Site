<br>
<textarea name="finish"  id="finish" placeholder="Результат здесь"></textarea>

<script>
function change() {
		var start = document.getElementById("start").value;
		var len = start.length;
		var result = new Array();
		for(i = 0; i < len; i++) {
			result[i] = switcher(start[i]);
		}
		document.getElementById("finish").value = result.join("");
}
alert("Хуй пизда сковорода - Вы нашли пасхалку");
